{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
    "allowImportExportEverywhere": true
  },
  "rules": {
    "arrow-parens": 0,
    "indent": [2, 2],
    "semi": [2, "never"],
    "space-before-function-paren": [2, "never"],
    "no-new": 0,
    "no-undef": 0
  },
  "globals": {
    "$": true
  }
}
